<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    
    <include layout="@layout/title_bar"/>
    
    <LinearLayout 
        
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/blue">
        <RelativeLayout 
		    android:id="@+id/RelativeLayout1"
		    android:layout_width="match_parent"
		    android:layout_height="match_parent"
		    android:background="@drawable/background"
		    android:orientation="vertical" >
		
		
		
		    <ImageButton
		        android:id="@+id/OutsideEetupBtn"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:layout_alignParentLeft="true"
		        android:layout_alignParentTop="true"
		        android:layout_marginLeft="60dp"
		        android:layout_marginTop="50dp"
		        android:src="@drawable/outsidesetup" />
		
		    <ImageButton
		        android:id="@+id/UsualCheckBtn"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:layout_alignParentRight="true"
		        android:layout_alignTop="@+id/OutsideEetupBtn"
		        android:layout_marginRight="60dp"
		        android:src="@drawable/usualcheck" />
		
		    <ImageButton
		        android:id="@+id/SettingBtn"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:layout_alignLeft="@+id/SpecialCheckBtn"
		        android:layout_below="@+id/SpecialCheckBtn"
		        android:layout_marginTop="60dp"
		        android:src="@drawable/setting" />
		
		    <ImageButton
		        android:id="@+id/ExitBtn"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:layout_alignLeft="@+id/LockCheck"
		        android:layout_below="@+id/LockCheck"
		        android:layout_marginTop="60dp"
		        android:src="@drawable/exit" />
		
		    <ImageButton
		        android:id="@+id/LockCheck"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:layout_alignLeft="@+id/UsualCheckBtn"
		        android:layout_below="@+id/UsualCheckBtn"
		        android:layout_marginTop="60dp"
		        android:src="@drawable/lockcheck" />
		
		    <ImageButton
		        android:id="@+id/SpecialCheckBtn"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:layout_alignLeft="@+id/OutsideEetupBtn"
		        android:layout_below="@+id/OutsideEetupBtn"
		        android:layout_marginTop="60dp"
		        android:src="@drawable/zhuanxiangjicha" />
		
		    <TextView
		        android:id="@+id/status"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:layout_alignLeft="@+id/OutsideEetupBtn"
		        android:layout_below="@+id/OutsideEetupBtn"
		        android:text="外勤装表"
		        android:textAppearance="?android:attr/textAppearanceMedium" />
		
		    <TextView
		        android:id="@+id/textView2"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:layout_alignLeft="@+id/UsualCheckBtn"
		        android:layout_below="@+id/UsualCheckBtn"
		        android:text="常规稽查"
		        android:textAppearance="?android:attr/textAppearanceMedium" />
		
		    <TextView
		        android:id="@+id/serverinfo"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:layout_alignLeft="@+id/SpecialCheckBtn"
		        android:layout_below="@+id/SpecialCheckBtn"
		        android:text="专项稽查"
		        android:textAppearance="?android:attr/textAppearanceMedium" />
		
		    <TextView
		        android:id="@+id/readinfo"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:layout_alignLeft="@+id/LockCheck"
		        android:layout_below="@+id/LockCheck"
		        android:text="封印验证"
		        android:textAppearance="?android:attr/textAppearanceMedium" />
		
		    <TextView
		        android:id="@+id/textView5"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:layout_alignLeft="@+id/SettingBtn"
		        android:layout_below="@+id/SettingBtn"
		        android:text="系统设置"
		        android:textAppearance="?android:attr/textAppearanceMedium" />
		
		    <TextView
		        android:id="@+id/textView6"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:layout_alignLeft="@+id/ExitBtn"
		        android:layout_below="@+id/ExitBtn"
		        android:text="注销登录"
		        android:textAppearance="?android:attr/textAppearanceMedium" />
		
		</RelativeLayout>
        
    </LinearLayout>

</LinearLayout>
